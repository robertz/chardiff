<h4>Character Info</h4>
<table class="table">
  <tr>
    <td width="40%">Name</td>
    <td>{{ cdata.name }}</td>
  </tr>
  <tr>
    <td>Class</td>
    <td>{{ cdata.class }}</td>
  </tr>
  <tr>
    <td>Level</td>
    <td>{{ cdata.level }}</td>
  </tr>
  <tr>
    <td>Paragon Level</td>
    <td>{{ cdata.paragonLevel }}</td>
  </tr>
  <tr>
    <td>Hardcore</td>
    <td>{{ cdata.hardcore }}</td>
  </tr>
  <tr>
    <td>Seasonal</td>
    <td>{{ cdata.seasonal }}</td>
  </tr>
  <tr>
    <td>Season Created</td>
    <td>{{ cdata.seasonCreated }}</td>
  </tr>
</table>

<h4>Character Stats</h4>
<table class="table">
  {{#each cdata.stats}}
  <tr>
    <td width="40%">{{@key}}</td>
    <td>{{ this }}</td>
  </tr>
  {{/each}}
</table>

<h4>Active Skills</h4>
<table class="table">
  <thead>
    <tr>
      <th>Slot</th>
      <th>Skill</th>
      <th>Rune</th>
    </tr>
  </thead>
  <tr>
    <td>1</td>
    <td>{{cdata.skills.active.0.skill.name}}</td>
    <td>{{cdata.skills.active.0.rune.name}}</td>
  </tr>
  <tr>
    <td>2</td>
    <td>{{cdata.skills.active.1.skill.name}}</td>
    <td>{{cdata.skills.active.1.rune.name}}</td>
  </tr>
  <tr>
    <td>3</td>
    <td>{{cdata.skills.active.2.skill.name}}</td>
    <td>{{cdata.skills.active.2.rune.name}}</td>
  </tr>
  <tr>
    <td>4</td>
    <td>{{cdata.skills.active.3.skill.name}}</td>
    <td>{{cdata.skills.active.3.rune.name}}</td>
  </tr>
</table>

<h4>Passive Skills</h4>
<table class="table">
  <thead>
    <tr>
      <th>Slot</th>
      <th>Skill</th>
    </tr>
  </thead>
  <tr>
    <td>1</td>
    <td>{{cdata.skills.passive.0.skill.name}}</td>
  </tr>
  <tr>
    <td>2</td>
    <td>{{cdata.skills.passive.1.skill.name}}</td>
  </tr>
  <tr>
    <td>3</td>
    <td>{{cdata.skills.passive.2.skill.name}}</td>
  </tr>
  <tr>
    <td>4</td>
    <td>{{cdata.skills.passive.3.skill.name}}</td>
  </tr>
</table>

<h4>Items</h4>
<table class="table">
  <thead>
    <tr>
      <th>Slot</th>
      <th>Item</th>
    </tr>
  </thead>
  <tr>
    <td><div class="item {{cdata.items.head.displayColor}}"><img src="//media.blizzard.com/d3/icons/items/large/{{{cdata.items.head.icon}}}.png" /></div></td>
    <td>{{cdata.items.head.name}}</td>
  </tr>
  <tr>
    <td><div class="item {{cdata.items.neck.displayColor}}"><img src="//media.blizzard.com/d3/icons/items/large/{{{cdata.items.neck.icon}}}.png" /></div></td>
    <td>{{cdata.items.neck.name}}</td>
  </tr>
  <tr>
    <td><div class="item {{cdata.items.shoulders.displayColor}}"><img src="//media.blizzard.com/d3/icons/items/large/{{{cdata.items.shoulders.icon}}}.png" /></div></td>
    <td>{{cdata.items.shoulders.name}}</td>
  </tr>
  <tr>
    <td><div class="item {{cdata.items.bracers.displayColor}}"><img src="//media.blizzard.com/d3/icons/items/large/{{{cdata.items.bracers.icon}}}.png" /></div></td>
    <td>{{cdata.items.bracers.name}}</td>
  </tr>
  <tr>
    <td><div class="item {{cdata.items.hands.displayColor}}"><img src="//media.blizzard.com/d3/icons/items/large/{{{cdata.items.hands.icon}}}.png" /></div></td>
    <td>{{cdata.items.hands.name}}</td>
  </tr>
  <tr>
    <td><div class="item {{cdata.items.torso.displayColor}}"><img src="//media.blizzard.com/d3/icons/items/large/{{{cdata.items.torso.icon}}}.png" /></div></td>
    <td>{{cdata.items.torso.name}}</td>
  </tr>
  <tr>
    <td><div class="item {{cdata.items.waist.displayColor}}"><img src="//media.blizzard.com/d3/icons/items/large/{{{cdata.items.waist.icon}}}.png" /></div></td>
    <td>{{cdata.items.waist.name}}</td>
  </tr>
  <tr>
    <td><div class="item {{cdata.items.legs.displayColor}}"><img src="//media.blizzard.com/d3/icons/items/large/{{{cdata.items.legs.icon}}}.png" /></div></td>
    <td>{{cdata.items.legs.name}}</td>
  </tr>
  <tr>
    <td><div class="item {{cdata.items.feet.displayColor}}"><img src="//media.blizzard.com/d3/icons/items/large/{{{cdata.items.feet.icon}}}.png" /></div></td>
    <td>{{cdata.items.feet.name}}</td>
  </tr>
  {{#if cdata.items.leftFinger.name}}
  <tr>
    <td><div class="item finger {{cdata.items.leftFinger.displayColor}}"><img src="//media.blizzard.com/d3/icons/items/large/{{{cdata.items.leftFinger.icon}}}.png" /></div></td>
    <td>{{cdata.items.leftFinger.name}}</td>
  </tr>
  {{/if}}
  {{#if cdata.items.rightFinger.name}}
  <tr>
    <td><div class="item finger {{cdata.items.rightFinger.displayColor}}"><img src="//media.blizzard.com/d3/icons/items/large/{{{cdata.items.rightFinger.icon}}}.png" /></div></td>
    <td>{{cdata.items.rightFinger.name}}</td>
  </tr>
  {{/if}}
  <tr>
    <td><div class="item {{cdata.items.mainHand.displayColor}}"><img src="//media.blizzard.com/d3/icons/items/large/{{{cdata.items.mainHand.icon}}}.png" /></div></td>
    <td>{{cdata.items.mainHand.name}}</td>
  </tr>
  {{#if cdata.items.offhand.name}}
  <tr>
    <td><div class="item {{cdata.items.offHand.displayColor}}"><img src="//media.blizzard.com/d3/icons/items/large/{{{cdata.items.offHand.icon}}}.png" /></div></td>
    <td>{{cdata.items.offHand.name}}</td>
  </tr>
  {{/if}}
</table>
